#include "asm.h"

int		translate(t_file *file)
{
	t_code	table;

	table = file->code_tab;
	while (table)
	{
		ft_printf(BLUE"%s:\n"E0M, table->label);
		//print intrinsic instructions (table->instr)
		table = table->next;
	}
	return (0);
}
