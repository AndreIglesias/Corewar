
#include "asm.h"

int		is_head(char ** cmd, char *str)
{
	int				quotes;
	unsigned int	i;

	i = 0;
	while (cmd[0][i] && i < ft_strlen(str))
	{
		if (cmd[0][i] != str[i])
			return (0);
		i++;
	}
	if (i != ft_strlen(str))
		return (0);
	if (ft_itersplit(cmd, i++) != '"')
		return (-1);
	quotes = 1;
	while (ft_itersplit(cmd, i))
	{
		if (ft_itersplit(cmd, i) == '"')
			quotes++;
		if (quotes >= 2 && ft_itersplit(cmd, i) == '#')
			break ;
		i++;
	}
	return ((quotes != 2) ? -1 : 1);
}

int		is_label(char **cmd)
{
	return (0);
}

int		is_opcode(char **cmd, int i)
{
	return (0);
}
